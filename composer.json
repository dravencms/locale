{
  "name": "dravencms/locale",
  "type": "dravencms-package",
  "description": "Locale module for dravencms",
  "keywords": [
    "dravencms",
    "locale"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "umpirsky/currency-list": "^1.0",
    "dravencms/application": ">=0.0.2",
    "dravencms/database": ">=0.0.3",
    "dravencms/base-form": ">=0.0.1",
    "dravencms/base-grid": ">=0.0.1",
    "dravencms/translation": ">=v1.0",
    "dravencms/location": ">=v1.1"
  },
  "suggest": {
    "dravencms/admin": "Provides admin inteface to manage locales",
    "dravencms/user": "User based locale and ACL settings"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": "dravencms.config.neon"
    }
  }
}
